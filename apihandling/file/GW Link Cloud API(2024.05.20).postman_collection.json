			"item": [
				{
					"name": "토큰 발급",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var response = JSON.parse(responseBody);\r",
									"\r",
									"if(response !== null){\r",
									"    tests[\"Is Success\"] = response.success === true;\r",
									"    if(response.success){\r",
									"        tests[\"Body contains access_token\"] = responseBody.has(\"access_token\");\r",
									"        pm.environment.set(\"acsToken\",response.access_token);\r",
									"        pm.environment.set(\"refToken\",response.refresh_token);\r",
									"    }\r",
									"    else{\r",
									"        var msg = \"code: \" + response.code + \" msg: \" + response.msg;\r",
									"        tests[msg] = false;\r",
									"    }\r",
									"}"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"protocolProfileBehavior": {
						"disabledSystemHeaders": {}
					},
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\r\n\t\"appid\" : \"GW Link 앱 로그인 아이디\",\r\n\t\"password\":\"GW Link 앱 로그인 비밀번호\",\r\n\t\"country_code\":\"82\"\r\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{url}}/gwapi/token/issue-tokens",
							"host": [
								"{{url}}"
							],
							"path": [
								"gwapi",
								"token",
								"issue-tokens"
							]
						}
					},
					"response": []
				},
				{
					"name": "토큰 새로고침",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var response = JSON.parse(responseBody);\r",
									"\r",
									"if(response !== null){\r",
									"    tests[\"Is Success\"] = response.success === true;\r",
									"    if(response.success){\r",
									"        tests[\"Body contains access_token\"] = responseBody.has(\"access_token\");\r",
									"        pm.environment.set(\"acsToken\",response.access_token);\r",
									"        pm.environment.set(\"refToken\",response.refresh_token);\r",
									"        pm.environment.set(\"uid\",response.uid);\r",
									"    }\r",
									"    else{\r",
									"        var msg = \"code: \" + response.code + \" msg: \" + response.msg;\r",
									"        tests[msg] = false;\r",
									"    }\r",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [
